{
    "openapi": "3.0.0",
    "info": {
        "title": "ReantalX Docs",
        "description": "This is an Api for ReantalX - Car's rent",
        "version": "1.0.0",
        "contact": {
            "name": "Gabriel Koch Fodi",
            "email": "gabrielkochf@gmail.com"
        }
    },
    "paths": {
        "/categories": {
            "post": {
                "tags":["Category"],
                "summary": "Create a category",
                "description":"Create a new category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "name":{
                                        "type":"string"
                                    },
                                    "description":{
                                        "type":"string"
                                    }
                                },
                                "example":{
                                    "name":"category name sample",
                                    "description":"category description sample"
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "201": {
                        "description":"Created"
                    },
                    "500":{
                        "description":"Category name is already being used"
                    }
                }
            },
            "get":{
                "tags":["Category"],
                "summary": "Listing all categories",
                "description": "Listing all objects from all categories",
                "responses": {
                    "200": {
                        "description":"Success",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "array",
                                    "items":{
                                        "type":"object",
                                        "properties":{
                                            "id":{
                                                "type": "string"
                                            },
                                            "name":{
                                                "type": "string"
                                            },
                                            "description":{
                                                "type": "string"
                                            },
                                            "created_at":{
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/categories/import": {
            "post": {
                "tags":["Category"],
                "summary": "Create a categories by csv",
                "description":"Create new categories by passing a dsv file",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "file":{
                                        "type":"string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "201": {
                        "description":"Created"
                    }
                }
            }
            },
        "/specification": {
            "post": {
                "tags":["Specification"],
                "summary": "Create a specification",
                "description":"Create a new specification",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "name":{
                                        "type":"string"
                                    },
                                    "description":{
                                        "type":"string"
                                    }
                                },
                                "example":{
                                    "name":"specification name sample",
                                    "description":"specification description sample"
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "201": {
                        "description":"Created"
                    },
                    "500":{
                        "description":"Specification name is already being used"
                    }
                }
            },
            "get":{
                "tags":["Specification"],
                "summary": "Listing all categories",
                "description": "Listing all objects from all categories",
                "responses": {
                    "200": {
                        "description":"Success",
                        "content": {
                            "application/json":{
                                "schema":{
                                    "type": "array",
                                    "items":{
                                        "type":"object",
                                        "properties":{
                                            "id":{
                                                "type": "string"
                                            },
                                            "name":{
                                                "type": "string"
                                            },
                                            "description":{
                                                "type": "string"
                                            },
                                            "created_at":{
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}